{"$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "title": {
      "text": "US GDP ",
      "subtitle":"by sector",
      "subtitleFontStyle":"italic",
      "subtitleFontSize":10,
      "anchor": "start",
      "color": "black"},
  
  "data":{"url": "https://raw.githubusercontent.com/ffatchen/ffatchen.github.io/main/ex.csv"},
  
  "height": 300,
  "width": 260,

  "mark": {
    "type": "bar",
    "color": "#86d0bb"},

"encoding": {
     "x": {
       "field": "Value",
       "type": "quantitative",
       "axis": {
         "title": null,
         "grid": false,
         "ticks": false,
         "labelAngle": 45}},

      "y": {
        "field": "shortName",
        "type": "nominal",
        "title": "",
        "axis": {"grid": false}}
}
}
